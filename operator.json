{
  "name": "Multiple Sequence Alignment operator",
  "description": "Performs Multiple Sequence Alignment in Tercen",
  "tags": ["bioinformatics"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/msa_operator"],
  "properties": [
   {
    "kind": "EnumeratedProperty",
    "name": "sequence_type",
    "defaultValue": "protein",
    "values": [
      "protein",
      "dna",
      "rna"
    ],
    "description": "dna, rna, or protein sequences?"
  },
  {
    "kind": "EnumeratedProperty",
    "name": "method",
    "defaultValue": "ClustalW",
    "values": [
      "ClustalW",
      "ClustalOmega",
      "Muscle"
    ],
    "description": "Alignment method, can be ClustalW, Muscle, or ClustalOmega."
  }
  ]
}


